using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace HomeWork1
{
    internal class Program
    {
        static void Main(string[] args)
        {
            int A = 0;
            int B = 0;

            do
            {
                B = int.Parse(Console.ReadLine());
                if (B > 0 && B % 2 == 1)
                    A += B;

            } while (B != 0);

            Console.WriteLine("A: " + A);
            Console.ReadLine();
        }
    }
}
